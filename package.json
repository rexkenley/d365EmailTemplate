{
  "name": "d365emailtemplate",
  "version": "1.0.0",
  "description": "A d365 CE package that would allow users to create visually rich email templates",
  "scripts": {
    "//NPM": "*****************************************************************",
    "buildDev": "npx eslint src && npx webpack --config webpack/config.dev.js",
    "buildDevChunk": "npx eslint src && npx webpack --config webpack/config.dev.chunk.js",
    "buildDevForm": "npx eslint src && npx webpack --config webpack/config.dev.form.js",
    "buildPrd": "npx webpack --config webpack/config.prd.js",
    "buildPrdChunk": "npx webpack --config webpack/config.prd.chunk.js",
    "buildPrdForm": "npx webpack --config webpack/config.prd.form.js",
    "//d365EmailTemplate": "*****************************************************************",
    "build:dev": "%npm_package_scripts_buildDevForm% src\\js\\.js -o dist\\.js",
    "build:prd": "%npm_package_scripts_buildPrdForm% src\\js\\.js -o dist\\.js"
  },
  "keywords": [
    "d365",
    "d365 ce",
    "email",
    "template"
  ],
  "author": "Rex Kenley Tan",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.6.3",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.3.5",
    "handlebars": "^4.5.1",
    "handlebars-intl": "^1.1.2",
    "npm-check": "^5.9.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-quill": "^1.3.3",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "eslint": "^6.6.0",
    "prettier": "^1.18.2",
    "webpack-cli": "^3.3.9"
  }
}
