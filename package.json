{
  "name": "d365emailtemplate",
  "version": "1.0.0",
  "description": "A d365 CE package that would allow users to create visually rich email templates",
  "scripts": {
    "//NPM": "*****************************************************************",
    "buildDev": "npx eslint src && npx webpack --config webpack/config.dev.js",
    "buildDevChunk": "npx eslint src && npx webpack --config webpack/config.dev.chunk.js",
    "buildDevForm": "npx eslint src && npx webpack --config webpack/config.dev.form.js",
    "buildPrd": "npx webpack --config webpack/config.prd.js",
    "buildPrdChunk": "npx webpack --config webpack/config.prd.chunk.js",
    "buildPrdForm": "npx webpack --config webpack/config.prd.form.js",
    "//d365EmailTemplate": "*****************************************************************",
    "build:dev": "%npm_package_scripts_buildDevForm% src\\js\\d365EmailTemplate.js -o dist\\d365EmailTemplate.js && npx pug src\\pug\\d365EmailTemplate.pug -o dist && %npm_package_scripts_buildDevForm% src\\js\\email.ribbon.js -o dist\\email.ribbon.js",
    "build:d365EmailTemplate:prd": "%npm_package_scripts_buildPrdForm% src\\js\\d365EmailTemplate.js -o dist\\d365EmailTemplate.js && npx pug src\\pug\\d365EmailTemplate.pug -o dist",
    "build:email:prd": "%npm_package_scripts_buildPrdForm% src\\js\\email.ribbon.js -o dist\\email.ribbon.js",
    "test": "npx jest --verbose ./test",
    "docs": "npx jsdoc -c jsdoc.json"
  },
  "keywords": [
    "d365",
    "d365 ce",
    "email",
    "template"
  ],
  "author": "Rex Kenley Tan",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.6.3",
    "@reduxjs/toolkit": "^1.0.4",
    "@tinymce/tinymce-react": "^3.3.2",
    "@types/jest": "^24.0.21",
    "@uifabric/icons": "^7.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.3.5",
    "handlebars": "^4.5.3",
    "handlebars-intl": "^1.1.2",
    "lodash": "^4.17.20",
    "office-ui-fabric-react": "^7.56.3",
    "pug": "^2.0.4",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "terser-webpack-plugin": "^2.2.1",
    "tinymce": "^5.1.1",
    "validator": "^12.0.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.3",
    "prettier": "^1.18.2",
    "pug-cli": "^1.0.0-alpha6",
    "style-loader": "^1.0.0",
    "webpack-cli": "^3.3.9"
  }
}
